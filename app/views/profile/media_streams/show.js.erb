$('.ruckus-modal-body-content').html('<%= j render 'edit' %>');
$('.ruckus-modal-body').mCustomScrollbar({ theme: 'dark-2' });

var html = '<%= j cell('media_stream/', profile: current_profile).call %>';
$('#media-block').html(html);
$('#media-slider').smoothTouchScroll({ continuousScrolling: false });
$('#media-slider .image-popup').magnificPopup($.extend({
  type: "image",
  mainClass: "mfp-img-mobile",
  image: {
    verticalFit: true
  }
}, self.defaultMFPOptions));

$('#media-slider .video-popup').magnificPopup($.extend({
  disableOn: 700,
  type: "iframe",
  preloader: false,
  fixedContentPos: false
}, self.defaultMFPOptions));
<%= render 'shared/profile/update_completion_percent' %>
